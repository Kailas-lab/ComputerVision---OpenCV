    "# Check if the image was loaded successfully\n",
    "\n",
    "cv2.imshow('image', re)\n",
    "cv2.waitKey(0)  # Wait for a key press to close the window\n",
    "cv2.destroyAllWindows()  # Close all OpenCV windows\n"
   ]
